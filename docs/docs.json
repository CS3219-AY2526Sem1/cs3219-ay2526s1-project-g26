{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "api": {
    "playground": {
      "proxy": false
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["index", "quickstart", "development", "deployment"]
          },
          {
            "group": "Implementations",
            "pages": [
              "implementations/peer-coding",
              "implementations/code-submission",
              "implementations/matching-algorithm"
            ]
          },
          {
            "group": "Customization",
            "pages": ["essentials/settings", "essentials/navigation"]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": ["api-reference/introduction"]
          },
          {
            "group": "UserService",
            "pages": [
              "api-reference/userservice/register-a-new-user",
              "api-reference/userservice/login-an-existing-user",
              "api-reference/userservice/verify-jwt-token-and-get-user-info",
              "api-reference/userservice/get-current-user-profile",
              "api-reference/userservice/update-user-profile"
            ]
          },
          {
            "group": "QuestionService",
            "pages": [
              "api-reference/questionservice/get-matching-question",
              "api-reference/questionservice/get-categories-and-difficulties",
              "api-reference/questionservice/get-question-by-id",
              "api-reference/questionservice/update-question",
              "api-reference/questionservice/delete-question",
              "api-reference/questionservice/get-question-test-cases",
              "api-reference/questionservice/get-all-questions",
              "api-reference/questionservice/create-question"
            ]
          },
          {
            "group": "HistoryService",
            "pages": [
              "api-reference/historyservice/get-user-submissions",
              "api-reference/historyservice/get-submission-details",
              "api-reference/historyservice/get-submission-status"
            ]
          },
          {
            "group": "AIService",
            "pages": [
              "api-reference/aiservice/translate-code"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
